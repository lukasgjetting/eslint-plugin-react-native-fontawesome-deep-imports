    "@types/node": "^17.0.22",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "mocha": "^9.2.2",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.6.2"
  },
  "bugs": {
    "url": "https://github.com/lukasgjetting/eslint-plugin-react-native-fontawesome-deep-imports/issues"
  },
  "homepage": "https://github.com/lukasgjetting/eslint-plugin-react-native-fontawesome-deep-imports#readme",
