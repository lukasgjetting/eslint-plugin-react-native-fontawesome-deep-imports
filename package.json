{
  "name": "eslint-plugin-react-native-fontawesome-deep-imports",
  "version": "1.0.0",
  "description": "An ESLint rule to enforce deep icon imports when using FontAwesome with React Native.",
  "main": "./dist/index.js",
  "author": "Lukas Gjetting",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasgjetting/eslint-plugin-react-native-fontawesome-deep-imports.git"
